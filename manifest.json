{
  "name": "Coursera Utilities",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Bunch of utilities that work with the Coursera site",
  "content_scripts": [
    {
      "matches": ["https://class.coursera.org/*"],
      "css": ["coursera-extension.css"],
      "js": ["jquery-1.10.2.min.js", "coursera-extension.js"],
      "run_at": "document_end"
    }
  ]

}

